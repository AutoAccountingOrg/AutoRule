{
  "categories": [
    {
      "title": "## 🚀 新功能",
      "labels": ["sparkles"]
    },
    {
      "title": "## 🌟 优化",
      "labels": ["art", "zap", "lipstick", "recycle"]
    },
    {
      "title": "## 🐛 修复",
      "labels": ["bug", "ambulance", "adhesive_bandage"]
    },
    {
      "title": "## 🔧 配置与脚本",
      "labels": ["wrench", "hammer"]
    },
    {
      "title": "## 🧪 测试",
      "labels": ["white_check_mark", "test_tube", "construction_worker"]
    },
    {
      "title": "## 📄 文档",
      "labels": ["memo", "pencil2", "page_facing_up"]
    },
    {
      "title": "## 📦 依赖",
      "labels": ["arrow_down", "arrow_up", "pushpin", "heavy_plus_sign", "heavy_minus_sign"]
    },
    {
      "title": "## 🔥 删除",
      "labels": ["fire", "coffin", "wastebasket"]
    },
    {
      "title": "## 🚀 部署",
      "labels": ["rocket"]
    },
    {
      "title": "## 💻 CI/CD",
      "labels": ["green_heart", "construction_worker"]
    },
    {
      "title": "## 🔐 安全",
      "labels": ["lock", "closed_lock_with_key", "passport_control", "safety_vest"]
    },
    {
      "title": "## 🏗 架构与基础设施",
      "labels": ["building_construction", "bricks", "technologist"]
    },
    {
      "title": "## 🌐 国际化与本地化",
      "labels": ["globe_with_meridians"]
    },
    {
      "title": "## 🔍 分析与优化",
      "labels": ["chart_with_upwards_trend", "mag"]
    },
    {
      "title": "## 🧹 代码清理",
      "labels": ["poop", "see_no_evil", "rewind", "boom"]
    },
    {
      "title": "## 🛠 开发者体验",
      "labels": ["technologist", "thread"]
    },
    {
      "title": "## 🎨 设计与 UI",
      "labels": ["lipstick", "dizzy", "iphone", "clown_face"]
    },
    {
      "title": "## 🧩 业务逻辑",
      "labels": ["necktie"]
    },
    {
      "title": "## 💬 文本与文案",
      "labels": ["speech_balloon"]
    },
    {
      "title": "## 🏷 标签",
      "labels": ["label"]
    }
  ],
  "template": "${{CHANGELOG}}",
  "pr_template": "- **${{TITLE}}**",
  "label_extractor": [
    {
      "pattern": ":(.*?): (.+)",
      "target": "$1",
      "on_property": "title"
    }
  ],
  "transformers": [
    {
      "pattern": ":(.*?): (.+)",
      "target": "$2"
    }
  ],
  "duplicate_filter": {
    "pattern": ".+",
    "on_property": "title",
    "method": "match"
  },
  "max_pull_requests": 1000,
  "max_back_track_time_days": 1000
}
